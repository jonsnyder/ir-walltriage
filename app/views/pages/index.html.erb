<h1>Listing pages</h1>

<table>
  <tr>
    <th>Facebook</th>
    <th>Name</th>
    <th>Photo url</th>
    <th>Username</th>
    <th>Can post</th>
    <th>Like count</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @pages.each do |page| %>
  <tr>
    <td><%= page.facebook_id %></td>
    <td><%= page.name %></td>
    <td><%= page.photo_url %></td>
    <td><%= page.username %></td>
    <td><%= page.can_post %></td>
    <td><%= page.like_count %></td>
    <td><%= link_to 'Show', page %></td>
    <td><%= link_to 'Edit', edit_page_path(page) %></td>
    <td><%= link_to 'Destroy', page, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Page', new_page_path %>
